pipeline
{
  agent any
  tools
  {
	maven 'maven3.9.0'
  }

  stages{
    stage ('checkoutcode')
    {
        steps
      {
             git branch: 'development', credentialsId: 'jenkins_credentials', url: 'https://github.com/maheshgamyadevops/maven-web-application.git'
      }
    }
	stage ('build')
    {
        steps
      {
             sh "mvn clean package"
      }
    }

    
  }
  
}
